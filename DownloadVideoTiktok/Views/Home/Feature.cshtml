@model DownloadVideoTiktok.Models.Article

@{
    ViewData["Title"] = Model.SEOTitle;
    ViewData["MetaTitle"] = Model.SEOTitle;
    ViewData["MetaDes"] = Model.SEODescrition;
    ViewData["MetaKeyword"] = Model.SEOKeywords;
}

<header class="masthead">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>Feature</h1>
                    <span class="subheading">@Html.Raw(Model.Summary)</span>
                </div>
            </div>
        </div>
    </div>
</header>
@await Html.PartialAsync("~/Views/Shared/_EditContentPartial.cshtml", Model)
@Html.Raw(Model.Detail)
